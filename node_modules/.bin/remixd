#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/remixd/src/bin/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/remixd/src/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/remixd/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/remixd/src/bin/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/remixd/src/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/remixd/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules/@remix-project/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/@remix-project+remixd@0.6.24/node_modules:/Volumes/N/CoopHive/hive/hardhat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-project/remixd/src/bin/remixd.js" "$@"
else
  exec node  "$basedir/../@remix-project/remixd/src/bin/remixd.js" "$@"
fi
